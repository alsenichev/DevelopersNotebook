<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:userControls="clr-namespace:DevelopersNotebook.UserControls">

    <userControls:SvgIcon x:Key="PlayIcon"
                          DataGeometry="{StaticResource Play}"
                          ShapeFill="{StaticResource IconsNeutral}"
                          IconWidth="30"
                          IconHeight="30"
                          Margin="5,3,1,3" />

    <userControls:SvgIcon x:Key="PauseIcon"
                          DataGeometry="{StaticResource Pause}"
                          ShapeFill="{StaticResource IconsNeutral}"
                          IconWidth="25"
                          IconHeight="25"
                          Margin="3" />

</ResourceDictionary>