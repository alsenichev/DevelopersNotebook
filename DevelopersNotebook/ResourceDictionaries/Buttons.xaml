<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:userControls="clr-namespace:DevelopersNotebook.UserControls">

    <userControls:SvgIcon x:Key="PlayIcon"
                          DataGeometry="{StaticResource Play}"
                          ShapeFill="{StaticResource IconsNeutral}"
                          IconWidth="10"
                          IconHeight="10"
                          Margin="2,3,0,2"
                          x:Shared="false"/>

    <userControls:SvgIcon x:Key="PauseIcon"
                          DataGeometry="{StaticResource Pause}"
                          ShapeFill="{StaticResource IconsNeutral}"
                          IconWidth="8"
                          IconHeight="8"
                          Margin="1"
                          x:Shared="false"/>

</ResourceDictionary>